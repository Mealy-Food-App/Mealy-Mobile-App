{
  "expo": {
    "name": "Mealy_App",
    "slug": "Mealy_App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "backgroundColor": "#e69f14",
      "image": "./src/assets/splash3.png",
      "resizeMode": "contain"
    },
    "developmentMode": false,
    "userInterfaceStyle": "light",
    "jsEngine": "hermes",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "jsEngine": "hermes",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#E69F14"
      },
      "package": "com.mally13.Mealy_App"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "96fc4f87-abea-4d5c-9055-032f522a3664"
      }
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Mealy to use your location."
        }
      ]
    ]
  },
  "name": "mealy_app"
}
